{
  "name": "ackee",
  "private": true,
  "version": "4.0.0a0",
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ],
  "description": "Self-hosted, Node.js based analytics tool for those who care about privacy",
  "main": "src/index.js",
  "type": "module",
  "exports": {
    ".": "./src/index.js"
  },
  "keywords": [
    "server",
    "tracking",
    "analytics"
  ],
  "license": "MIT",
  "homepage": "https://github.com/electerious/Ackee",
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/Ackee.git"
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/electerious"
  },
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "NODE_ENV=development nodemon",
    "build": "node build.js",
    "test": "ava",
    "test:cov": "nyc npm test",
    "coverage": "nyc report --reporter=lcov",
    "lint": "eslint \"{functions,src,test}/**/*.js\""
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@apollo/server-plugin-landing-page-graphql-playground": "^4.0.1",
    "@graphql-tools/merge": "^9.0.19",
    "ackee-tracker": "^5.1.0",
    "apollo-server-lambda": "^3.13.0",
    "apollo-server-plugin-http-headers": "^0.1.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "debounce-promise": "^3.1.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-scalars": "^1.24.1",
    "graphql-tag": "^2.12.6",
    "is-url": "^1.2.4",
    "is-valid-domain": "^0.1.6",
    "micro": "^10.0.1",
    "microrouter": "^3.1.3",
    "mongoose": "^8.10.1",
    "node-schedule": "^2.1.1",
    "normalize-url": "^8.0.1",
    "pino": "^9.6.0",
    "request-ip": "^3.3.0",
    "sanitize-filename": "^1.6.3",
    "signale": "^1.4.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@apollo/client": "^3.13.1",
    "@electerious/eslint-config": "^4.1.0",
    "ava": "6.2.0",
    "classnames": "^2.5.1",
    "formbase": "^12.0.2",
    "history": "^5.3.0",
    "human-number": "^2.0.4",
    "mocked-env": "^1.3.5",
    "mongodb-memory-server": "^10.1.3",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "normalize.css": "^8.0.1",
    "nyc": "^17.1.0",
    "pino-pretty": "^13.0.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-apollo-network-status": "^5.3.1",
    "react-dom": "^19.0.0",
    "react-fast-compare": "^3.2.2",
    "react-hotkeys-hook": "^4.6.1",
    "react-use": "^17.6.0",
    "rosid-handler-js-next": "^1.0.1",
    "rosid-handler-sass": "^8.0.0",
    "s-ago": "^2.2.0",
    "shortid": "^2.2.17",
    "test-listen": "^1.1.0",
    "url-pattern": "^1.0.3"
  },
  "ava": {
    "verbose": true,
    "timeout": "20s"
  },
  "nodemonConfig": {
    "exec": "npm run build && npm start | pino-pretty",
    "ext": "js,json,graphql,scss",
    "watch": [
      "./src"
    ]
  },
  "engines": {
    "node": ">= 20.11"
  }
}